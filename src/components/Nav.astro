---
const links = [
  {
    href: '#',
    name: 'Inicio',
  },
  {
    href: '#',
    name: 'Valores',
  },
  {
    href: '#',
    name: 'Proyectos',
  },
  {
    href: '#',
    name: 'Eventos',
  },
  {
    href: '#',
    name: 'Contribuyentes',
  },
]

const socialLinks = [
  {
    href: '#',
    name: 'github',
  },
  {
    href: '#',
    name: 'discord',
  },
]
---

<nav class="absolute top-5 z-50 flex items-center justify-between gap-5 rounded-full bg-white/30 p-4">
  <img src="/logo.png" alt="" />
  {links.map((link) => <a href={link.href}>{link.name}</a>)}
  {
    socialLinks.map((link) => (
      <a href={link.href} class="size-8">
        <img src={`./social/${link.name}.png`} alt={link.name} />
      </a>
    ))
  }
</nav>
